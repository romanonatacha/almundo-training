{
  "name": "almundo-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "i18n": "ng xi18n",
    "dev": "run-p -l dev:server dev:client",
    "dev:client": "ng serve",
    "dev:server": "gulp server:dev",
    "build:client": "node scripts/build.js",
    "build:server": "gulp server:build",
    "test": "run-s test:server test:client",
    "test:client": "ng test",
    "test:server": "jest --config src/server/jest.config.json --coverage",
    "posttest:client": "mv coverage/test-report.xml coverage/client/test-report.xml",
    "posttest:server": "mv coverage/test-report.xml coverage/server/test-report.xml",
    "start": "node build/bin/www.js",
    "lint": "ng lint",
    "release": "standard-version"
  },
  "private": true,
  "dependencies": {
    "@am/bunyan-logger-builder": "~2.0.0",
    "@am/core": "~1.11.1",
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@sentry/browser": "~5.6.3",
    "axios": "~0.19.0",
    "body-parser": "~1.19.0",
    "cookie-parser": "~1.4.3",
    "cors": "~2.8.5",
    "express": "~4.17.1",
    "express-healthcheck": "~0.1.0",
    "method-override": "~3.0.0",
    "newrelic": "~6.2.0",
    "pug-inject-express-engine": "~1.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "~8.2.0",
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.0",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "~8.1.0",
    "@commitlint/config-conventional": "~8.1.0",
    "@ngrx/schematics": "~8.2.0",
    "@ngrx/store": "~8.2.0",
    "@types/body-parser": "~1.17.0",
    "@types/bunyan": "~1.8.5",
    "@types/cookie-parser": "~1.4.1",
    "@types/cors": "~2.8.1",
    "@types/express": "~4.17.1",
    "@types/jest": "~24.0.18",
    "@types/method-override": "~0.0.31",
    "@types/newrelic": "~5.11.0",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "gulp": "~4.0.0",
    "gulp-cli": "~2.2.0",
    "gulp-nodemon": "~2.4.2",
    "gulp-typescript": "~5.0.0",
    "husky": "~3.0.5",
    "jest": "~24.9.0",
    "jest-sonar-reporter": "~2.0.0",
    "lint-staged": "~9.2.5",
    "npm-run-all": "~4.1.5",
    "prettier": "~1.18.2",
    "standard-version": "~7.0.0",
    "stylelint": "~12.0.0",
    "stylelint-config-recommended-scss": "~4.1.0",
    "stylelint-order": "~3.1.0",
    "stylelint-scss": "~3.13.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  },
  "prettier": {
    "printWidth": 90,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{scss,css}": [
      "stylelint --syntax scss --fix",
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "stylelint": {
    "plugins": [
      "stylelint-order"
    ],
    "extends": "stylelint-config-recommended-scss",
    "rules": {
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true
    }
  },
  "jestSonar": {
    "reportPath": "coverage"
  },
  "schematicsVersion": "2.0.2"
}
